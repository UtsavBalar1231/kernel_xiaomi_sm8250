# Device Specific Board Configuration
CONFIG_BOARD_CMI=y

# Touchscreen
CONFIG_TOUCHSCREEN_ST_FTS_V521=y
CONFIG_TOUCHSCREEN_FOD=y
# CONFIG_TOUCHSCREEN_ST_DEBUG_FS is not set
CONFIG_SECURE_TOUCH=y

# Fingerprint
CONFIG_FINGERPRINT_GOODIX_FOD=y

# Power supply drivers
# CONFIG_QPNP_FG_GEN4 is not set
CONFIG_IDT_P9415=y
CONFIG_RX1619=y
CONFIG_RX1619_TRIM=y
CONFIG_LN8282=y
CONFIG_HL6111R=y
CONFIG_FUEL_GAUGE_BQ27Z561=y
CONFIG_BQ2597X_CHARGE_PUMP=y
CONFIG_BQ_PUMP_WIRELESS_CHARGE=y
