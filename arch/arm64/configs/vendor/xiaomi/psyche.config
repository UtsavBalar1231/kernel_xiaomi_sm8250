# Device Specific Board Configuration
CONFIG_BOARD_PSYCHE=y

# Touchscreen
CONFIG_FTS_FOD_AREA_REPORT=y
CONFIG_TOUCHSCREEN_ST_FTS_V521_SPI=y
CONFIG_TOUCHSCREEN_FOD=y
# CONFIG_TOUCHSCREEN_ST_DEBUG_FS is not set
CONFIG_TOUCHSCREEN_SUPPORT_NEW_GAME_MODE=y

# Fingerprint
CONFIG_FINGERPRINT_GOODIX_FOD=y

# Enable PM8008 Regulator
CONFIG_REGULATOR_PM8008=y

# Enable MI US Proximity sensor
CONFIG_US_PROXIMITY=y

# Enable GPIO Testing driver
CONFIG_GPIO_TESTING_MODE=y

# Power supply drivers
# CONFIG_QPNP_FG_GEN4 is not set
# CONFIG_QPNP_SMB5 is not set
CONFIG_QPNP_SMB5_PSYCHE=y
CONFIG_CHARGER_LN8000_PSYCHE=y
CONFIG_FUEL_GAUGE_BQ27Z561_PSYCHE=y
