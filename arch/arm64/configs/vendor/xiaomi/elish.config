# Device Specific Board Configuration
CONFIG_BOARD_ELISH=y

# Touchscreen
CONFIG_TOUCHSCREEN_SUPPORT_NEW_GAME_MODE=y
CONFIG_TOUCHSCREEN_NT36523_HOSTDL_SPI=y
# CONFIG_TOUCHSCREEN_NVT_DEBUG_FS is not set

# Fingerprint
CONFIG_FINGERPRINT_GOODIX_TA=y

# Enable PM8008 Regulator
CONFIG_REGULATOR_PM8008=y

# Power supply drivers
# CONFIG_QPNP_FG_GEN4 is not set
CONFIG_IDT_P9418=y
CONFIG_DUAL_FUEL_GAUGE_BQ27Z561=y
CONFIG_BQ2597X_CHARGE_PUMP=y
CONFIG_DUAL_BQ2597X=y
CONFIG_CHARGER_LN8000=y

# Enable ELISH specifc geni serial driver
# CONFIG_SERIAL_MSM_GENI is not set
CONFIG_SERIAL_MSM_GENI_ELISH=y

# Enable KTZ Semiconductor KTZ8866 backlight driver 
CONFIG_BACKLIGHT_KTZ8866=y

# Disable ESOC
# CONFIG_ESOC is not set

# Enable Xiaomi keyboard
CONFIG_XIAOMI_KEYBOARD=y

# Retry i2c operations on failure
CONFIG_I2C_RETRY=y
