			<135 512 50000 1800000>,
			<1 825 30000 131000>,
			/* 50 MB/s */
			<150 10073 50000 790000>,
			<135 512 80000 1800000>,
			<1 825 40000 131000>,
			/* 100 MB/s */
			<150 10073 50000 790000>,
			<135 512 100000 1800000>,
			<1 825 50000 131000>,
			/* 200 MB/s */
